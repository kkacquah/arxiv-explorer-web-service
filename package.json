{
  "name": "ontology-web-service",
  "version": "1.0.0",
  "description": "An API built with Node.js/Hapi.js that follows DDD + Clean Architecture principles",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon",
    "start": "node index.js",
    "test": "jest",
    "coverage": "jest --coverage"
  },
  "author": "Kenneth Acquah",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kkacquah/arxiv-explorer-web-service.git"
  },
  "dependencies": {
    "@hapi/boom": "^8.0.1",
    "@hapi/good": "^8.2.4",
    "@hapi/good-console": "^8.1.2",
    "@hapi/good-squeeze": "^5.2.1",
    "@hapi/hapi": "^19.0.0",
    "@hapi/inert": "^5.2.2",
    "@hapi/joi": "^16.1.8",
    "@hapi/vision": "^5.5.4",
    "@hapi/yar": "^10.0.0",
    "airtable": "^0.8.1",
    "blipp": "^4.0.1",
    "dotenv": "^8.2.0",
    "hapi": "^18.1.0",
    "hapi-limiter": "^0.1.1",
    "hapi-rate-limit": "^4.1.0",
    "hapi-rate-limiter": "^3.0.1",
    "hapi-rate-limitor": "^3.0.0",
    "hapi-swagger": "^10.3.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "node-fetch": "^2.6.0",
    "require-directory": "^2.1.1",
    "sequelize": "^5.21.3"
  },
  "devDependencies": {
    "jest": "^24.9.0",
    "nodemon": "^1.19.4",
    "sqlite3": "^4.1.1"
  },
  "engines": {
    "node": ">=12",
    "npm": ">=6.12"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{js}",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/coverage/**"
    ],
    "testURL": "http://localhost/"
  },
  "bugs": {
    "url": "https://github.com/jbuget/nodejs-clean-architecture-app/issues"
  },
  "homepage": "https://github.com/jbuget/nodejs-clean-architecture-app#readme",
  "directories": {
    "doc": "doc",
    "lib": "lib",
    "test": "test"
  },
  "keywords": [
    "heroku",
    "test"
  ],
  "heroku-run-build-script": true
}
